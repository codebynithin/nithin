{
  "name": "nithin",
  "version": "1.0.0",
  "private": true,
  "author": {
    "name": "Nithin Viswanathan",
    "email": "mails2nithin@gmail.com"
  },
  "scripts": {
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "start:web": "react-scripts start",
    "build": "react-scripts build",
    "test:web": "react-scripts test",
    "eject:web": "react-scripts eject",
    "start:server": "cd server && node index.js",
    "dev:server": "cd server && nodemon index.js",
    "build:server": "pm2 start server/index.js",
    "start": "npm install && concurrently \"npm run start:web\" \"npm run start:server\"",
    "start:dev": "npm install && concurrently \"npm run start:web\" \"npm run dev:server\""
  },
  "dependencies": {
    "@testing-library/dom": "~10.4.0",
    "@testing-library/jest-dom": "~6.6.3",
    "@testing-library/react": "~16.3.0",
    "@testing-library/user-event": "~13.5.0",
    "cors": "~2.8.5",
    "dotenv": "~16.0.3",
    "express": "~4.18.2",
    "mongoose": "~7.0.3",
    "primeflex": "^4.0.0",
    "primereact": "^10.9.6",
    "react": "~19.1.0",
    "react-dom": "~19.1.0",
    "react-router-dom": "~6.30.1",
    "react-scripts": "5.0.1",
    "sass": "~1.89.2",
    "web-vitals": "~2.1.4"
  },
  "devDependencies": {
    "@types/jest": "~30.0.0",
    "@types/react": "~19.1.8",
    "@types/react-dom": "~19.1.6",
    "concurrently": "~9.2.0",
    "eslint": "~8.57.1",
    "husky": "~4.2.5",
    "lint-staged": "~11.0.0",
    "nodemon": "~3.0.1",
    "prettier": "~2.3.0",
    "typescript": "~4.9.5"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "lint-staged": {
    "{server,src}/app/**/*.(tsx|ts)": [
      "eslint --fix"
    ],
    "{server,src}/**/*.(html|scss|json|tsx|ts)": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
