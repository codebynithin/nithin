{
  "name": "nithin",
  "version": "1.1.0",
  "private": true,
  "author": {
    "name": "Nithin Viswanathan",
    "email": "mails2nithin@gmail.com"
  },
  "homepage": "https://codebynithin.com",
  "scripts": {
    "predeploy": "GENERATE_SOURCEMAP=false npm run build",
    "deploy": "gh-pages -d build",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "start": "PUBLIC_URL=/ react-scripts start",
    "build": "PUBLIC_URL=https://codebynithin.com react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "dependencies": {
    "@testing-library/jest-dom": "~6.6.3",
    "@testing-library/react": "~16.3.0",
    "@testing-library/user-event": "~13.5.0",
    "lucide-react": "~0.539.0",
    "react": "~19.1.0",
    "react-dom": "~19.1.0",
    "react-router-dom": "~6.30.1",
    "react-scripts": "5.0.1",
    "web-vitals": "~2.1.4"
  },
  "devDependencies": {
    "@commitlint/cli": "~12.1.4",
    "@commitlint/config-angular": "~12.1.4",
    "@types/jest": "~30.0.0",
    "@types/react": "~19.1.8",
    "@types/react-dom": "~19.1.6",
    "autoprefixer": "~10.4.20",
    "eslint": "~8.57.1",
    "gh-pages": "~6.3.0",
    "husky": "~4.2.5",
    "lint-staged": "~11.0.0",
    "postcss": "~8.5.6",
    "postcss-nesting": "~13.0.2",
    "postcss-scss": "~4.0.9",
    "prettier": "~2.3.0",
    "sass": "~1.93.2",
    "tailwindcss": "~3.4.17",
    "typescript": "~4.9.5"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "lint-staged": {
    "src/app/**/*.(tsx|ts|js|jsx)": [
      "eslint --fix"
    ],
    "src/**/*.(html|scss|json|tsx|ts|js|jsx)": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  }
}
